<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <application>
        <action-listener>
            org.primefaces.application.DialogActionListener
        </action-listener>
        <navigation-handler>
            org.primefaces.application.DialogNavigationHandler
        </navigation-handler>
        <view-handler>
            org.primefaces.application.DialogViewHandler
        </view-handler>
    </application>     
    <converter>
        <converter-id>activeSubgroupConverter</converter-id>
        <converter-class>jm.com.dpbennett.business.entity.converter.ActiveSubgroupConverter</converter-class>
    </converter>
    <converter>
        <converter-id>activeDepartmentConverter</converter-id>
        <converter-class>jm.com.dpbennett.business.entity.converter.ActiveDepartmentConverter</converter-class>
    </converter>
    <converter>
        <converter-id>businessOfficeConverter</converter-id>
        <converter-class>jm.com.dpbennett.business.entity.converter.BusinessOfficeConverter</converter-class>
    </converter>
    <converter>
        <converter-id>activeEmployeeConverter</converter-id>
        <converter-class>jm.com.dpbennett.business.entity.converter.ActiveEmployeeConverter</converter-class>
    </converter>
    <converter>
        <converter-id>serviceConverter</converter-id>
        <converter-class>jm.com.dpbennett.business.entity.converter.ServiceConverter</converter-class>
    </converter> 
    <converter>
        <converter-id>clientContactConverter</converter-id>
        <converter-class>jm.com.dpbennett.business.entity.converter.ClientContactConverter</converter-class>
    </converter> 
    <converter>
        <converter-id>businessConverter</converter-id>
        <converter-class>jm.com.dpbennett.business.entity.converter.BusinessConverter</converter-class>
    </converter> 
    <validator>
        <validator-id>accountingCodeValidator</validator-id>
        <validator-class>jm.com.dpbennett.business.entity.validator.AccountingCodeValidator</validator-class>
    </validator>  
    <validator>
        <validator-id>systemOptionValidator</validator-id>
        <validator-class>jm.com.dpbennett.business.entity.validator.SystemOptionValidator</validator-class>
    </validator>   
    <validator>
        <validator-id>clientValidator</validator-id>
        <validator-class>jm.com.dpbennett.business.entity.validator.ClientValidator</validator-class>
    </validator>
    <validator>
        <validator-id>jobClientValidator</validator-id>
        <validator-class>jm.com.dpbennett.business.entity.validator.JobClientValidator</validator-class>
    </validator>
    <validator>
        <validator-id>generalClientValidator</validator-id>
        <validator-class>jm.com.dpbennett.business.entity.validator.GeneralClientValidator</validator-class>
    </validator>
    <validator>
        <validator-id>requiredFieldValidator</validator-id>
        <validator-class>jm.com.dpbennett.business.entity.validator.RequiredFieldValidator</validator-class>
    </validator>
    <validator>
        <validator-id>businessEntityValidator</validator-id>
        <validator-class>jm.com.dpbennett.business.entity.validator.BusinessEntityValidator</validator-class>
    </validator>
    <validator>
        <validator-id>jobNumberValidator</validator-id>
        <validator-class>jm.com.dpbennett.business.entity.validator.JobNumberValidator</validator-class>
    </validator>
    <validator>
        <validator-id>subcontractedDepartmentValidator</validator-id>
        <validator-class>jm.com.dpbennett.business.entity.validator.SubcontractedDepartmentValidator</validator-class>
    </validator>
    <validator>
        <validator-id>addressValidator</validator-id>
        <validator-class>jm.com.dpbennett.business.entity.validator.AddressValidator</validator-class>
    </validator>
    <validator>
        <validator-id>turnaroundTimeValidator</validator-id>
        <validator-class>jm.com.dpbennett.business.entity.validator.TurnaroundTimeValidator</validator-class>
    </validator>
    <validator>
        <validator-id>addressLineValidator</validator-id>
        <validator-class>jm.com.dpbennett.business.entity.validator.AddressLineValidator</validator-class>
    </validator>
    <validator>
        <validator-id>contactValidator</validator-id>
        <validator-class>jm.com.dpbennett.business.entity.validator.ContactValidator</validator-class>
    </validator> 
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>employeeDialog</from-outcome>
            <to-view-id>/hrm/employeeDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>  
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>departmentDialog</from-outcome>
            <to-view-id>/hrm/departmentDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>    
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>departmentPickListDialog</from-outcome>
            <to-view-id>/hrm/departmentPickListDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>    
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>subgroupPickListDialog</from-outcome>
            <to-view-id>/hrm/subgroupPickListDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule> 
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>businessDialog</from-outcome>
            <to-view-id>/hrm/businessDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule> 
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>subgroupDialog</from-outcome>
            <to-view-id>/hrm/subgroupDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>     
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>divisionDialog</from-outcome>
            <to-view-id>/hrm/divisionDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <lifecycle>
        <phase-listener id="nocache">jm.com.dpbennett.jmts.utils.CacheControlPhaseListener</phase-listener>
    </lifecycle>
    
